generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Service_Priority {
  id               BigInt   @id(map: "PK_Service_Priority") @default(autoincrement())
  service_provider String?
  priority         BigInt
  service          String?
  is_active        Boolean
  updated_at       DateTime @db.Timestamptz(6)
}

model EFMigrationsHistory {
  MigrationId    String @id(map: "PK___EFMigrationsHistory") @db.VarChar(150)
  ProductVersion String @db.VarChar(32)

  @@map("__EFMigrationsHistory")
}

model application_log {
  id              BigInt   @id(map: "PK_application_log") @default(autoincrement())
  created_date    DateTime @db.Timestamptz(6)
  user_code       String?  @db.Uuid
  request_url     String?
  log             String?
  log_type        String?
  log_stack_trace String?
  request         String?
  token           String?
}

model communication_block_list {
  id             BigInt   @id(map: "PK_communication_block_list") @default(autoincrement())
  mobile_number  String?
  email          String?
  is_active      Boolean
  created_at     DateTime @db.Timestamptz(6)
  last_update_at DateTime @db.Timestamptz(6)
}

model email_log {
  id                   BigInt   @id(map: "PK_email_log") @default(autoincrement())
  created_at           DateTime @db.Timestamptz(6)
  log_type             String
  log                  String
  netcore_message      String?
  netcore_message_id   String?
  netcore_Submitted_at String?
  netcore_status       String?
  bouncereasonid       BigInt?
  customargs1          String?
  customargs2          String?
  email                String?
  fromaddress          String?
  ip                   String?
  msize                BigInt?
  subject              String?
  tags                 String?
  type                 String?
  url                  String?
  useragent            String?
}

model email_templates {
  id         BigInt   @id(map: "PK_email_templates") @default(autoincrement())
  body       String?
  body_type  String?
  is_active  Boolean
  subject    String?
  created_at DateTime @db.Timestamptz(6)
}

model hangfire_logs {
  id          BigInt   @id(map: "PK_hangfire_logs") @default(autoincrement())
  scheduler   String?
  createdAt   DateTime @db.Timestamptz(6)
  type        String?
  description String?
}

model push_notification_log {
  id              BigInt   @id(map: "PK_push_notification_log") @default(autoincrement())
  created_at      DateTime @db.Timestamptz(6)
  log_type        String?
  log             String?
  message_id      String?
  response_id     String?
  response_status String?
  service         String?
  email           String?
  mobile_number   String?
}

model push_notification_token {
  id              BigInt   @id(map: "PK_push_notification_token") @default(autoincrement())
  user_code       String   @db.Uuid
  token           String?
  created_at      DateTime @db.Timestamp(6)
  last_updated_at DateTime @db.Timestamp(6)
  is_active       Boolean?
}

model sms_log {
  id                   BigInt   @id(map: "PK_sms_log") @default(autoincrement())
  created_at           DateTime @db.Timestamptz(6)
  log_type             String
  log                  String
  netcore_status       String?
  netcore_request_id   String?
  netcore_submitted_at String?
  feedid               BigInt?  @default(0)
  service_provider     String?
  service_provider_id  String?
}

model sms_mobile_request {
  id            BigInt  @id(map: "PK_sms_mobile_request") @default(autoincrement())
  mobile_number String?
  request_id    String?
}

model sms_template {
  id            BigInt   @id(map: "PK_sms_template") @default(autoincrement())
  template_id   String?
  template      String?
  credential_id BigInt
  is_active     Boolean
  created_at    DateTime @db.Timestamptz(6)
}

model sms_template_credentials {
  id         BigInt   @id(map: "PK_sms_template_credentials") @default(autoincrement())
  feed_id    String?
  user_name  String?
  password   String?
  entity_id  String?
  is_active  Boolean
  sms_url    String?
  created_at DateTime @db.Timestamptz(6)
}

model whatsapp_log {
  id                   BigInt   @id(map: "PK_whatsapp_log") @default(autoincrement())
  created_at           DateTime @db.Timestamptz(6)
  log_type             String
  log                  String
  netcore_message      String?
  netcore_message_id   String?
  netcore_Submitted_at String?
  netcore_status       String?
  mobile_number        String?
  source               String?
}

model whatsapp_template {
  id                     BigInt   @id(map: "PK_whatsapp_template") @default(autoincrement())
  template               String?
  template_id            String?
  template_credential_id BigInt
  is_active              Boolean
  created_at             DateTime @db.Timestamptz(6)
}

model whatsapp_template_credentials {
  id             BigInt   @id(map: "PK_whatsapp_template_credentials") @default(autoincrement())
  url            String?
  message_type   String?
  recipient_type String?
  source         String?
  x_apiheader    String?
  is_active      Boolean
  created_at     DateTime @db.Timestamptz(6)
}
